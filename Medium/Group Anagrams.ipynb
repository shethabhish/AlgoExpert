   "source": [
    "from collections import defaultdict\n",
    "def group_anagrams(words):\n",
    "    result = defaultdict(list)\n",
    "    for word in words:\n",
    "        result[tuple(sorted(word))].append(word)\n",
    "    return result.values()"
   ]
