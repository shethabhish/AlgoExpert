   "source": [
    "def number_of_ways(arr,amount):\n",
    "    dp = [0]*(amount+1)\n",
    "    dp[0] = 1\n",
    "    for change in arr:\n",
    "        for i in range(1,amount+1):\n",
    "            if change <= amount:\n",
    "                dp[i] += dp[i-change]\n",
    "    return dp[amount]"
   ]
