   "source": [
    "def product_array(nums):\n",
    "    temp = 1\n",
    "    result = []\n",
    "    for i in nums:\n",
    "        result.append(temp)\n",
    "        temp *= i\n",
    "    temp = 1\n",
    "    for i in range(len(nums)-1,-1,-1):\n",
    "        result[i] *= temp\n",
    "        temp *= nums[i]\n",
    "    return result"
   ]
