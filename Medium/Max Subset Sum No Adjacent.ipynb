    "def max_subset(arr):\n",
    "    dp = [0]*len(arr)\n",
    "    dp[0] = arr[0]\n",
    "    dp[1] = max(arr[0],arr[1])\n",
    "    for i in range(2,len(arr)):\n",
    "        dp[i] = max(dp[i-1],dp[i-2]+arr[i])\n",
    "    return dp[-1]"
   ]
