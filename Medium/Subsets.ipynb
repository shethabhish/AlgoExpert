    "def subsets(arr):\n",
    "    subset = []\n",
    "    subset.append([])\n",
    "    for num in arr:\n",
    "        for i in range(len(subset)):\n",
    "            sub = list(subset[i])\n",
    "            sub.append(num)\n",
    "            subset.append(sub)\n",
    "    return subset"
   ]
