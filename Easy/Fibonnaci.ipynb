    "def fib(num,nums):\n",
    "    if num == 1 or num == 2:\n",
    "        return 1\n",
    "    if num in nums:\n",
    "        return nums[num]\n",
    "    result = fib(num-1,nums) + fib(num-2,nums)\n",
    "    nums[num] = result\n",
    "    return result"
   ]
  },
